<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saga">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.flash" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.InicioActivity"/>
        <activity android:name=".activity.ListaUniversalActivity" />
        <activity android:name=".activity.ConferenciaItemNotaEntradaActivity" />
        <activity android:name=".activity.CadastroEmbalagemActivity" />
        <activity android:name=".activity.ConfiguracaoActivity" />
        <activity android:name=".activity.LocacaoProdutoActivity" />
        <activity android:name=".activity.ListaProdutoActivity" />
        <activity android:name=".activity.LocacaoEnderecoActivity" />
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
